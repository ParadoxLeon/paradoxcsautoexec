//DUCK JUMP
alias "+hjump" "+jump; +duck"
alias "-hjump" "-jump; -duck"
bind "b" "+hjump"
//MOUSE BINDS
bind "mouse5" "r_cleardecals"
bind mwheelup "+jump"
bind mwheeldown "+jump"
//SCORENET
net_graphpos "2"
net_graphheight "1"
alias +scorenet "net_graph 1; +showscores "
alias -scorenet "net_graph 0; -showscores"
bind tab "+scorenet"
//JUMP TROW
bind "alt" "+jump;-attack;-jump"
//NADE BINDS
bind "f" "use weapon_knife; use weapon_flashbang"
bind "x" "use weapon_knife; use weapon_smokegrenade"
bind "q" "use weapon_knife; use weapon_molotov;use weapon_incgrenade"
bind "g" "use weapon_knife; use weapon_hegrenade"
bind "k" "ignorerad; ignoremsg; toggle voice_enable 0 1;"
//VEIWMODEL
viewmodel_fov "65"
viewmodel_offset_x "2"
viewmodel_offset_y "2"
viewmodel_offset_z "-2"
viewmodel_presetpos "3"
cl_bob_lower_amt "5"
cl_bobamt_lat "0.1"
cl_bobamt_vert "0.1"
cl_bobcycle "0.98"
cl_viewmodel_shift_left_amt "0.5"
cl_viewmodel_shift_right_amt "0.25"

//END
bind "end" "toggleconsole"
